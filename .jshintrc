{
	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"eqeqeq": true,
	"es3": true,
	"forin": true,
	"freeze": true,
	"immed": true,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonbsp": true,
	"nonew": true,
	"plusplus": false,
	"undef": true,
	"unused": true,
	"strict": true,
	"trailing": true,

	"indent": 2,
	"quotmark": "single",

	"maxcomplexity": 6,
	"maxstatements": 12,
	"maxparams": 4,
	"maxdepth": 3,
	"maxlen": 100,
	"maxerr": 50,

	"globals": {
		"define": true,
		"module": true
	}
}