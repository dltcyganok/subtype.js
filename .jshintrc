{
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "eqnull": true,
  "es3": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": false,
  "strict": true,
  "trailing": true,
  "undef": true,
  "unused": true,

  "quotmark": "single",
  "indent": 2,
  "maxcomplexity": 10,
  "maxstatements": 12,
  "maxparams": 4,
  "maxdepth": 3,
  "maxlen": 100,
  "maxerr": 50,

  "predef": [
    "module",
    "define"
  ]
}